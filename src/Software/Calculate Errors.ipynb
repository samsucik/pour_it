   "outputs": [],
   "source": [
    "def calculate_error(values,target):\n",
    "    sum_ = 0\n",
    "    for value in values:\n",
    "        sum_ += (target - value) ** 2\n",
    "    return sum_/len(values)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [
