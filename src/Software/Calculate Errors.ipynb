    "def read_normal_values():\n",
    "    f = open('new_reads_normal.txt','r')\n",
    "    values = []\n",
    "    for line in f:\n",
    "        if(line == 'Failsafe delimiter string \\n'):\n",
    "            continue\n",
    "        else:\n",
    "            values.append(float(line[:-2]))\n",
    "    return values"
   ]
  },
