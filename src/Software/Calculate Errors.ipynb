    "def extract_int(string):\n",
    "    digits = []\n",
    "    for c in string:\n",
    "        if(str.isdigit(c)):\n",
    "            digits.append(int(c))\n",
    "    if(len(digits) == 2):\n",
    "        return 10 + digits[1]\n",
    "    return digits[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def read_NN_values():\n",
    "    dict_ = {}\n",
    "    f = open('new_reads_NN.txt','r')\n",
    "    values = []\n",
    "    key = -1\n",
    "    for line in f:\n",
    "        if(line.startswith('Epoch')):\n",
    "            if(len(values) != 0):\n",
    "                dict_[key] = values\n",
    "                values = []\n",
    "            key = extract_int(line)\n",
    "        else:\n",
    "            values.append(float(line[:-2]))\n",
    "    dict_[key] = values\n",
    "    return dict_"
   ]
  },
